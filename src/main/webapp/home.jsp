<%@ page language="java" contentType="text/html; charset=UTF-8" pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Home</title>
    <link rel="stylesheet" type="text/css" href="css/home.css">
</head>
<body>
<div class="home">
    <!-- Banner Section -->
    <div class="banner">
        <div class="banner-image">
            <img src="images/bannermain.png" alt="Banner">
        </div>
    </div>

    <!-- Categories Section -->
    <div class="categories">
        <h2>Categories</h2>
        <div class="category-boxes">
            <a href="category-furniture" class="category-box">
                <img src="images/furniture.png" alt="Furniture">
                <p>Furniture</p>
            </a>
            <a href="category-clothing" class="category-box">
                <img src="images/cloth.png" alt="Clothing">
                <p>Clothing</p>
            </a>
            <a href="category-decor" class="category-box">
                <img src="images/decor.png" alt="Decoration">
                <p>Decoration</p>
            </a>
            <a href="category-watches" class="category-box">
                <img src="images/watch.png" alt="Accessories">
                <p>Accessories</p>
            </a>
            <a href="category-music" class="category-box">
                <img src="images/music.png" alt="Music">
                <p>Music</p>
            </a>
        </div>
    </div>
</div>
</body>
</html>
